<!DOCTYPE html>
<html>
<head>
  <title>AppWithUserAuth</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
</head>
<body>

<% if session[:user_id] %>
<h2 class='User_Header'> <%="LOGGED IN AS #{User.find(session[:user_id]).username}"%> <h2>
<% end %>


<%= yield %>

</body>
</html>
